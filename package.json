{
  "name": "federated-memory",
  "version": "0.1.0",
  "description": "Federated memory system with Central Memory Index for LLMs",
  "deploy": "2025-01-18-emergency-auth",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node -r module-alias/register dist/index.js",
    "start:prod": "NODE_ENV=production node -r module-alias/register dist/index.js",
    "start:railway": "(npx prisma migrate deploy || npx prisma db push) && (node -r module-alias/register dist/scripts/init-db.js || true) && npm run start:prod",
    "start:minimal": "node dist/minimal-health.js",
    "start:debug": "node dist/minimal-health.js",
    "start:ultra": "node dist/ultra-minimal.js",
    "test": "jest",
    "test:unit": "jest --testPathPattern=unit",
    "test:integration": "jest --testPathPattern=integration",
    "test:e2e": "jest --testPathPattern=e2e",
    "test:coverage": "jest --coverage",
    "test:setup": "tsx scripts/setup-test-db.ts",
    "test:module": "tsx scripts/test-technical-module.ts",
    "test:api": "tsx scripts/test-rest-api.ts",
    "test:mcp": "tsx scripts/test-mcp-server.ts",
    "test:mcp:quick": "./test-mcp-quick.sh",
    "test:mcp:interactive": "node test-mcp-interactive.js",
    "test:email": "tsx scripts/test-email.ts",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json}\"",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:migrate:deploy": "prisma migrate deploy",
    "db:init": "tsx scripts/init-db.ts",
    "db:verify": "tsx scripts/verify-db.ts",
    "cmi:populate": "tsx scripts/populate-cmi-index.ts",
    "db:seed": "tsx prisma/seed.ts",
    "db:studio": "prisma studio",
    "db:reset": "prisma migrate reset",
    "generate:module": "tsx scripts/generate-module.ts",
    "migrate:bigmemory": "tsx scripts/migrate-bigmemory-sql.ts",
    "migrate:test": "./scripts/test-migration-local.sh",
    "migrate:backup": "./scripts/backup-databases.sh",
    "migrate:check": "tsx scripts/check-bigmemory-db.ts"
  },
  "keywords": [
    "memory",
    "llm",
    "mcp",
    "pgvector",
    "federated",
    "ai"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.15.1",
    "@prisma/client": "^5.7.1",
    "@types/express-session": "^1.18.2",
    "@types/nodemailer": "^6.4.17",
    "@types/passport": "^1.0.17",
    "@types/passport-github2": "^1.2.9",
    "@types/passport-google-oauth20": "^2.0.16",
    "bcrypt": "^5.1.1",
    "bcryptjs": "^3.0.2",
    "connect-redis": "^9.0.0",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "dotenv": "^16.3.1",
    "eventsource": "^3.0.7",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-session": "^1.18.1",
    "helmet": "^7.1.0",
    "ioredis": "^5.3.2",
    "jsonwebtoken": "^9.0.2",
    "module-alias": "^2.2.3",
    "node-fetch": "^2.7.0",
    "nodemailer": "^7.0.5",
    "openai": "^4.24.1",
    "passport": "^0.7.0",
    "passport-github2": "^0.1.12",
    "passport-google-oauth20": "^2.0.0",
    "pg": "^8.16.3",
    "pgvector": "^0.1.5",
    "redis": "^5.6.0",
    "uuid": "^9.0.1",
    "winston": "^3.11.0",
    "ws": "^8.16.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.11",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.10.6",
    "@types/pg": "^8.15.4",
    "@types/uuid": "^9.0.7",
    "@types/ws": "^8.5.10",
    "@typescript-eslint/eslint-plugin": "^6.18.0",
    "@typescript-eslint/parser": "^6.18.0",
    "axios": "^1.10.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "jest": "^29.7.0",
    "prettier": "^3.1.1",
    "prisma": "^5.7.1",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "_moduleAliases": {
    "@": "dist",
    "@core": "dist/core",
    "@modules": "dist/modules",
    "@api": "dist/api",
    "@services": "dist/services",
    "@utils": "dist/utils",
    "@config": "dist/config"
  }
}
